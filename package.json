{
  "name": "vsvirus",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "test": "jest",
    "lint": "echo 'please install ESLint in your code editor: https://eslint.org/'",
    "clean": "npx rimraf ./.next",
    "seed": "ts-node --project ./scripts/tsconfig.json scripts/seed",
    "start": "next start --port $PORT",
    "storybook": "start-storybook -p 6006",
    "prisma:migrate:save": "prisma migrate save --experimental",
    "prisma:migrate:up": "prisma migrate up --experimental",
    "prisma:migrate:down": "prisma migrate down --experimental",
    "prisma:migrate:saveAndUpdate": "yarn prisma:migrate:save && yarn prisma:migrate:up && yarn prisma:generate",
    "prisma:introspect": "prisma introspect",
    "postinstall": "prisma generate",
    "prisma:generate": "prisma generate",
    "build-storybook": "build-storybook",
    "graphql:schema-download": "npx apollo schema:download --endpoint=http://localhost:3000/api/graphql ./src/graphql-schema.json",
    "generate:types": "yarn graphql:schema-download && npx apollo codegen:generate --localSchemaFile=src/graphql-schema.json --target=typescript --includes=\"src/**/*.{tsx,ts}\" --tagName=gql --globalTypesFile=src/types/global-types.ts types",
    "devpost:import": "ts-node --project ./scripts/tsconfig.json scripts/devpost/import"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "plopTemplates"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.0.0-rc.4",
    "@apollo/link-context": "^2.0.0-beta.3",
    "@apollo/react-ssr": "4.0.0-beta.1",
    "@material-ui/core": "^4.9.7",
    "@nexus/schema": "^0.14.1",
    "@prisma/cli": "^2.0.0",
    "@prisma/client": "^2.0.0",
    "@ra-data-prisma/backend": "^1.1.0",
    "@ra-data-prisma/dataprovider": "^1.2.1",
    "apollo-server": "^2.11.0",
    "apollo-server-micro": "^2.11.0",
    "browser-image-resizer": "^2.1.0",
    "common-tags": "^1.8.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "crypto": "^1.0.1",
    "csvtojson": "^2.0.10",
    "easy-peasy": "^3.3.0",
    "env-cmd": "^10.1.0",
    "google-spreadsheet": "^3.0.10",
    "graphql": "^14.6.0",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.2.1",
    "graphql-tag": "^2.10.3",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.15",
    "memoizee": "^0.4.14",
    "moment": "^2.24.0",
    "next": "^9.4.4",
    "nexus-prisma": "^0.14.0",
    "node-fetch": "^2.6.0",
    "nodemailer": "^6.4.8",
    "nodemailer-mailgun-transport": "^2.0.0",
    "papaparse": "^5.1.1",
    "pluralize": "^8.0.0",
    "react": "16.13.1",
    "react-admin": "^3.3.3",
    "react-dom": "16.13.1",
    "react-dropzone": "^10.2.1",
    "react-image-lightbox": "^5.1.1",
    "react-linkify": "^1.0.0-alpha",
    "rimraf": "^3.0.2",
    "simpl-schema": "^1.5.7",
    "string-capitalize-name": "^1.0.3",
    "styled-components": "^5.0.1",
    "styled-reset": "^4.1.2",
    "ts-node": "^8.8.1",
    "uniforms": "^2.6.6",
    "uniforms-bridge-simple-schema-2": "^2.6.6",
    "uniforms-material": "^2.6.6"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/addons": "^5.3.17",
    "@storybook/react": "^5.3.17",
    "@types/csvtojson": "^1.1.5",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.2",
    "@types/nodemailer": "^6.4.0",
    "@types/simpl-schema": "^0.2.6",
    "apollo": "^2.28.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "jest": "^25.2.3",
    "typescript": "^3.8.3"
  }
}
